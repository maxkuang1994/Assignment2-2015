<header style="background:#009BD9;height:100vh">
  <section class="hero-three-items" style="padding-top:0px;padding-bottom:0px">
    <article class="hero-block primary c6" style="width:100%;height:100vh">
      <a aria-hidden="true" class="img-link c2 page-scroll" href="#main"style="background-image:url(/img/two.png)">

      </a>
      <div class="intro-text text-box" style="padding-top:50px; padding-bottom: 40px;color:#ffffff;  width: 100%;">
        <div class="intro-heading">EXPLORE</div>
        <div class="intro-lead-in">Love Together</div>
      </div>
    </article>
  </section>
</header>

<div style="background:#F7F6F6;"id="main">
  <section id="portfolio" style="overflow:hidden;margin:0 auto">
    <div class="container aa">
      <div>
        <section class="hero-three-items" style="padding-top:0px;padding-bottom:0px">
          <article class="hero-block primary c6" style="width:100%;">
            <a aria-hidden="true" class="img-link c2 page-scroll" style="background-image:url(/img/one.png)">

            </a>

            <div class="text-box c8">
              <h5 style="text-align:left">
            <a class="c4 noTextDec" style="word-wrap: break-word;color:#ffffff">
            We exlore the best places and helps you make your dating more memorable.<br><br>Cultivate your relationship with your partner. 
            </a>
            </h5>
            </div>
          </article>


        </section>
      </div>
    </div>

    <div class="container bb">
      <div style=" padding-bottom: 1.5em">

        <p style=" color:#ccc;font-weight: 600;  font-style: italic; font-size:1.5em"></p>

        <a class="btn-danger btn-xl" style="border:none;border-radius:0px" href="/auth/instagram">Instagram</a>
      </div>
      <div>

        <div>

          <p style="width: 32%;font-size:1.4em;font-style: italic; color:#ccc;display:inline-block;cursor:pointer" class="span2" tabindex="0">Fashion</p>

          <p style="width: 32%;font-size:1.4em;font-style: italic; color:#ccc;display:inline-block;cursor:pointer" class="span3" tabindex="0">Health</p>

          <p style="width: 32%;font-size:1.4em;font-style: italic; color:#ccc;display:inline-block;cursor:pointer" class="span4" tabindex="0">Travel</p>
          <ol data-view="results" class="headlines touchstates hp alert2" style="text-align:left;font-size:1.1em;list-style-type: decimal-leading-zero;">
            {{#each healthnews}} {{#if image}}
            <li class="border-b highlighted  item-0">
              <a href="{{url}}" class="noTextDoc greenhover">
             {{title}}
              </a>
            </li>{{/if}} {{/each}}
          </ol>


          <ol data-view="results" class="headlines touchstates hp alert3" style="text-align:left;font-size:1.1em;list-style-type: decimal-leading-zero;">
            {{#each healthnews2}} {{#if image}}
            <li class="border-b highlighted  item-0">
                          <a href="{{url}}" class="noTextDoc greenhover">
  {{title}}
              </a>
            </li>{{/if}} {{/each}}
          </ol>


          <ol data-view="results" class="headlines touchstates hp alert4" style="text-align:left;font-size:1.1em;list-style-type: decimal-leading-zero;">
            {{#each healthnews3}} {{#if image}}
            <li class="border-b highlighted  item-0">
                          <a href="{{url}}" class="noTextDoc greenhover">
  {{title}}
              </a>
            </li>{{/if}} {{/each}}
          </ol>

        </div>
      </div>

    </div>

  </section>
</div>



<div>
  <style type="text/css">

    body {
      background-color: #00A5E7;
      background-image: url(http://www.transparenttextures.com/patterns/bright-squares.png);
      margin: 0 auto;
      font-family: Consolas, sans-serif;
      font-size: 1.5em;
      color: #454545;
      font-weight: bold;
      text-align: center;
      margin: 0 auto
    }
    svg {
      stroke: #000000;
      stroke-width: 3px;
      stroke-opacity: 0.4;
      fill: rgba(255, 255, 255, 0.29);
      background: url(/img/four.png) no-repeat fixed 100% 100%;
    }
    text {
      stroke-width: 1em;
      stroke: #000000;
      fill: #000000;
    }
    circle {
      fill: #FF8533;
      stroke-width: 1.1em;
      stroke: rgb(223, 94, 94);
    }
    circle:hover {
      fill: #f24e17;
      stroke-width: 1.5px;
      stroke: rgb(210, 38, 38);
    }
    a:hover,
    a {
      text-decoration: none;
    }
  </style>

<script type="text/javascript">
var width = 1000,
    height = 700;

 // Create a projection of the US
//var projection = d3.geo.albersUsa()
  //  .scale(1000)
   // .translate([width/2, height/2]);


    var projection = d3.geo.transverseMercator()
		.rotate([117, -33])
   		.scale(20000);

var path = d3.geo.path().projection(projection);

// Create the SVG that state paths and location points will be attached to
var d3_map = d3.select("body").append("svg:svg")
  .attr("width", "100%")
  .attr("height", height)
  .attr("style","text-align:center");

// Create a 'g' element within the SVG which state paths will be attached to
var states = d3_map.append("svg:g")
  .attr("class", "states");
        
var locations = d3_map.append("svg:g")
  .attr("class", "locations");



var mapFeatures = d3.json("/css/map.json", function (collection) {
    states.selectAll("path")
      .data(collection.features)
      .enter().append("path")
      .attr("d", path); 
 });
var coordinate =[];
coordinate.coordinates = [];



{{#each yelpResults}}

 var coordinates=[];

  coordinate.push({
       label:"{{name}};"+
               " "+
              "Rating:{{rating}}",
       coordinates:[{{longitude}},{{latitude}}],
       url:"{{image_url}}",
       });

{{/each}}



var tooltip = d3.selectAll("body")
							.append("a")
							.style("position", "absolute")
							.style("z-index", "50")
							.style("visibility", "hidden")
							.text("a simple tooltip");
             

var locations = d3.select(".locations").selectAll('circle').data(coordinate);

locations.enter().append("svg:circle")
    .attr("cy", function(d) { return projection(d.coordinates)[1];})
	  .attr("cx", function(d) { return projection(d.coordinates)[0];})
    .attr("id", function(d) { return "aa"})
    .attr("r", 4.5)
    .on("mouseover", function(d){return tooltip.text(d.label).style("visibility","visible").style("background","#ffffff").style("opacity","0.8") .attr("href",d.url);})
							.on("mousemove", function(){return tooltip.style("top", (event.pageY-10)+"px").style("left",(event.pageX+10)+"px");})
						//	.on("mouseout", function(){return tooltip.style("visibility", "hidden");})
							
    </script>
</div>


